{
    "name": "OWA Mail Notification",
    "version": "1.0.0.0",
    "description": "Simple notifications for new Mails in OWA",
    "manifest_version": 2,
    "optional_permissions": [],
    "permissions": ["storage" , "system.cpu", "alarms", "http://*/*", "https://*/"],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "",
    "web_accessible_resources": []
  }